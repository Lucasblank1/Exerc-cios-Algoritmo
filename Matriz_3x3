var
i,j:inteiro
num:vetor[1..3, 1..3] de inteiro
inicio
// Seção de Comandos
Escreval("Entre com os valores da matriz A ")

Para i de 1 ate 3 faca
   para j de 1 ate 3 faca
      Leia (num[i,j])
   fimpara
   escreva()
fimpara

Escreval()
      Escreval("Matriz A")
      para i de 1 ate 3 faca
         para j de 1 ate 3 faca
            Escreva (num [i,j])
         Fimpara
         Escreval()
      Fimpara
      escreval()
Para i de 1 ate 3 faca
   para j de 1 ate 3 faca
      Se i = j entao
         escreva(num[i,j])
      fimse
Fimpara
